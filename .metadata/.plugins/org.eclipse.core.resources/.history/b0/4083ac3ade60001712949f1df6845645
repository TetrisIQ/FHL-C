#include "ringbuffer.h"
#include <stdio.h>

int main(void) {

    rbinit(10);
    rbput('A');
    rbput('B');
    rbput('C');
    printf("%c", rbget());
    rbput('D');
    printf("%c", rbget());
    rbput('E');
    printf("%c", rbget());
    rbput('F');
    printf("%c", rbget());
    rbfree();
}

