int anacae(char* text) {

}

char findTheE(char* text, int textSize) {
    int countArray[122]; //122 ist z
    int i;
    char copy[textSize] ;
    char c;
    strcpy(copy, text);
    for (int i = 0; copy[i] != '\0'; i++) {
        c = toupper(copy[i]);
        countArray[c]++;
    }

}


int StringLength(char* text) {
        int i = 0;
        while (text[i] != '\0') {
                i++;
        }
        return i;
}

